{
    "comments": {
        "lineComment": "//"
    },
    "brackets": [
        ["(", ")"],
        ["[", "]"],
        ["{", "}"]
    ],
    "autoClosingPairs": [
        ["(", ")"],
        ["[", "]"],
        ["{", "}"],
        ["\"", "\""]
    ],
    "folding": {
        "markers": {
            "start": "\\{",
            "end": "\\}"
        }
    },
    "wordPattern": "[^ \\t\\n\\r%$\\(\\)\\{\\}]+"
}